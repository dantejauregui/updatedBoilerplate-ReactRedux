{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "import",
    "jsx-a11y",
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "rules": {
        "react/prefer-stateless-function": 0,
        "react/jsx-indent": 0,
        "comma-dangle": ["error", {"functions": "ignore"}],
        "react/jsx-indent-props": 0.
    }
}
